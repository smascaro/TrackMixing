<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="InheritingTextSwitcher">
        <attr name="childrenTextSize" format="dimension" />
        <attr name="childrenTextColor" format="color" />
    </declare-styleable>

    <declare-styleable name="VerticalSeekbar">
        <attr name="showNumericProgress" format="boolean" />
        <attr name="suffixText" format="string" />
        <attr name="preffixText" format="string" />
        <attr name="progress_position" format="enum">
            <enum name="center_fixed" value="0" />
            <enum name="dynamic_trespass" value="1" />
            <!-- The dynamic_no_trespass mode is the equivalent to not set a trespass_mode -->
            <enum name="dynamic_no_trespass" value="2" />
        </attr>
        <attr name="trespass_mode" format="flags">
            <flag name="start" value="1" />
            <flag name="end" value="2" />
        </attr>
    </declare-styleable>
</resources>